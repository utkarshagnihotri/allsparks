<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    cq:template="/apps/aemmobile/templates/issue"
    jcr:description="A content container for an App managed using Experience Manager Mobile based on Geometrixx Unlimited"
    jcr:primaryType="cq:Template"
    jcr:title="AEM Mobile runtime - customized"
    allowedPaths="[/content/phonegap(/.*)?,/content/mobileapps(/.*)?]"
    pge-type="app-publish"
    wizard="/libs/mobileapps/dps/admin/content/createpublishappsteps.html">
    <jcr:content
        cq:allowedTemplates="[/apps/aemmobile/templates/.*]"
        cq:designPath="/etc/designs/aemmobile"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aemmobile/components/pages/issue"
        dps-exportTemplate="/etc/contentsync/templates/dps-geometrixx-unlimited-app"
        dps-resourceType="dps:Project"
        pge-dashboard-config="/libs/mobileapps/dps/admin/content/dashboard/tiles/dpsdefault"
        pge-type="[app-group]"/>
</jcr:root>
