<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Template with focus on content."
    jcr:primaryType="cq:Template"
    jcr:title="aemmobile Site Content Page with Logo"
    ranking="{Long}100">
    <jcr:content
        cq:designPath="/etc/designs/aemmobile"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aemmobile/components/structure/page"
        dps-resourceType="dps:Article">
        <logo
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemmobile/components/structure/logo"/>
        <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="aemmobile/components/content/title"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aemmobile/components/content/text"
                text="&lt;p>This is a content page&lt;/p>"
                textIsRich="true"/>
        </par>
    </jcr:content>
</jcr:root>
